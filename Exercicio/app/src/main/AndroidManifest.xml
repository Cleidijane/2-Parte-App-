<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aluno.exercicio">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListarActivity"></activity>
        <activity android:name=".Iniciar" />
        <activity android:name=".Ranking" />
        <activity
            android:name=".Questionario"
            android:label="@string/title_activity_questionario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NovasQuestoes"
            android:label="@string/title_activity_novas_questoes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Creditos"
            android:label="@string/title_activity_creditos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Perguntas"
            android:label="@string/title_activity_perguntas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Salas" />
    </application>

</manifest>